<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox prefHeight="400.0" prefWidth="472.0" xmlns="http://javafx.com/javafx/8.0.101" xmlns:fx="http://javafx.com/fxml/1" fx:controller="photos.controller.AdminController">
  <children>
    <AnchorPane maxHeight="-1.0" maxWidth="-1.0" prefHeight="-1.0" prefWidth="-1.0" VBox.vgrow="ALWAYS">
      <children>
            <TableView fx:id="tableView" layoutX="36.0" layoutY="28.0" prefHeight="348.0" prefWidth="200.0">
              <columns>
                <TableColumn fx:id="tableColumn" prefWidth="199.0" resizable="false" text="Usernames" />
              </columns>
            </TableView>
            <Button fx:id="deleteButton" layoutX="281.0" layoutY="109.0" mnemonicParsing="false" onAction="#deleteAction" prefHeight="63.0" prefWidth="131.0" text="Delete User">
               <font>
                  <Font name="System Bold" size="18.0" />
               </font>
            </Button>
            <Button fx:id="addButton" layoutX="281.0" layoutY="28.0" mnemonicParsing="false" onAction="#addAction" prefHeight="63.0" prefWidth="131.0" text="Add User">
               <font>
                  <Font name="System Bold" size="18.0" />
               </font>
            </Button>
            <Button fx:id="cancelButton" disable="true" layoutX="281.0" layoutY="322.0" mnemonicParsing="false" onAction="#cancelAction" text="Cancel" />
            <Button fx:id="logoutButton" layoutX="281.0" layoutY="189.0" mnemonicParsing="false" onAction="#logoutAction" prefHeight="63.0" prefWidth="131.0" text="Logout">
               <font>
                  <Font name="System Bold" size="18.0" />
               </font>
            </Button>
            <TextField fx:id="userField" disable="true" layoutX="272.0" layoutY="283.0" />
            <Button fx:id="add2Button" disable="true" layoutX="360.0" layoutY="322.0" mnemonicParsing="false" onAction="#add2Action" text="Add" />
      </children>
    </AnchorPane>
  </children>
</VBox>
