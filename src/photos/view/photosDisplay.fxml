<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox prefHeight="437.0" prefWidth="640.0" xmlns="http://javafx.com/javafx/8.0.101" xmlns:fx="http://javafx.com/fxml/1" fx:controller="photos.controller.photosDisplayController">
  <children>
    <AnchorPane maxHeight="-1.0" maxWidth="-1.0" prefHeight="-1.0" prefWidth="-1.0" VBox.vgrow="ALWAYS">
      <children>
            <Button fx:id="nextButton" layoutX="365.0" layoutY="356.0" mnemonicParsing="false" onAction="#nextImage" prefHeight="49.0" prefWidth="139.0" text="Next&gt;&gt;">
               <font>
                  <Font name="System Bold" size="16.0" />
               </font>
            </Button>
            <ImageView fx:id="featured" fitHeight="322.0" fitWidth="431.0" layoutX="105.0" layoutY="14.0" pickOnBounds="true" preserveRatio="true" />
            <Button fx:id="prevButton" layoutX="133.0" layoutY="356.0" mnemonicParsing="false" onAction="#previousImage" prefHeight="49.0" prefWidth="139.0" text="&lt;&lt;Previous">
               <font>
                  <Font name="System Bold" size="16.0" />
               </font>
            </Button>
      </children>
    </AnchorPane>
  </children>
</VBox>
