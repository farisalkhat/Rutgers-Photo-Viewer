<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox prefHeight="205.0" prefWidth="426.0" xmlns="http://javafx.com/javafx/8.0.101" xmlns:fx="http://javafx.com/fxml/1" fx:controller="photos.controller.photosLoginController">
  <children>
    <AnchorPane maxHeight="-1.0" maxWidth="-1.0" prefHeight="176.0" prefWidth="453.0" VBox.vgrow="ALWAYS">
      <children>
            <Label layoutX="15.0" prefHeight="122.0" prefWidth="396.0" text="Welcome to Faris and Miguel's Photo Viewer!&#10;Please sign in." textAlignment="CENTER">
               <font>
                  <Font name="System Bold" size="18.0" />
               </font>
            </Label>
            <TextField fx:id="loginField" layoutX="166.0" layoutY="115.0" text="username" />
            <Label layoutX="56.0" layoutY="117.0" prefHeight="21.0" prefWidth="110.0" text="Username:">
               <font>
                  <Font name="System Bold" size="18.0" />
               </font>
            </Label>
            <Button fx:id="loginButton" layoutX="185.0" layoutY="160.0" mnemonicParsing="false" onAction="#handleLoginAction" text="Login" />
      </children>
    </AnchorPane>
  </children>
</VBox>
